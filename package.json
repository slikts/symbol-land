    "@types/node": "^12.6.8",
    "jest": "^24.8.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.5.3"
  },
  "prettier": {
    "trailingComma": "all"
  },
